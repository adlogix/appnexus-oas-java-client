<?xml version="1.0" encoding="UTF-8"?>
<AdXML>
  <Request type="CampaignGroup">
    <Database action="add">
      <CampaignGroup>
        <Id>$groupId$</Id>
$if(groupDescription)$        <Description>$groupDescription$</Description>$endif$
$if(notes)$        <Notes>$notes$</Notes>$endif$
$if(externalUserIds)$        <ExternalUsers>
          $externalUserIds:{u|<UserId>$u$</UserId>}; separator="\n"$
        </ExternalUsers>$endif$
        <InternalQuickReport>$internalQuickReport$</InternalQuickReport>
        <ExternalQuickReport>$externalQuickReport$</ExternalQuickReport>
      </CampaignGroup>
    </Database>
  </Request>
</AdXML>